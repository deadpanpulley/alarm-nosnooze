{
  "expo": {
    "owner": "advickbhalla",
    "name": "alarmy",
    "slug": "alarmy-advickbhalla",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.alarmy",
      "infoPlist": {
        "UIBackgroundModes": [
          "audio",
          "fetch",
          "processing",
          "remote-notification"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "softwareKeyboardLayoutMode": "pan",
      "package": "com.anonymous.alarmy",
      "permissions": [
        "RECEIVE_BOOT_COMPLETED",
        "VIBRATE",
        "WAKE_LOCK",
        "SCHEDULE_EXACT_ALARM",
        "USE_EXACT_ALARM",
        "USE_FULL_SCREEN_INTENT",
        "FOREGROUND_SERVICE",
        "SYSTEM_ALERT_WINDOW",
        "android.permission.RECEIVE_BOOT_COMPLETED",
        "android.permission.WAKE_LOCK"
      ],
      "allowBackup": true,
      "useNextNotificationsApi": true
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "newArchEnabled": false,
    "githubUrl": "https://github.com/Pankajtanwarbanna/alarmy",
    "extra": {
      "eas": {
        "projectId": "e9e857bc-04b7-4aad-842d-3f58b805cfaf"
      }
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "sounds": [
            "./assets/alarm.mp3"
          ],
          "mode": "production"
        }
      ],
      [
        "expo-background-fetch",
        {
          "minimumInterval": 60
        }
      ],
      [
        "expo-task-manager"
      ],
      "expo-build-properties"
    ],
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/e9e857bc-04b7-4aad-842d-3f58b805cfaf"
    }
  }
}
